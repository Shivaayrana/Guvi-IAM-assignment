    <Reference class="sailpoint.object.Bundle" name="abc_bank_IT_role"/>
  </Requirements>
  <Selector>
    <IdentitySelector>
      <MatchExpression>
        <MatchTerm name="employeeType" type="IdentityAttribute" value="Employee"/>
      </MatchExpression>
    </IdentitySelector>
  </Selector>
</Bundle>